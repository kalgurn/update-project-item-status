{
  "name": "update-project-item-status",
  "version": "0.0.3",
  "private": true,
  "description": "TypeScript action to change GitHub Projects items status",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalgurn/update-project-item-status.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Kostiantyn Kulbachnyi",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.16.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.5.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
